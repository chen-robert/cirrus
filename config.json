{
  "langs": {
    "c": {
      "compile": {
        "cmd": "gcc",
        "opts": ["-std=c11", "-Wall", "-Wextra", "-O3", "-lm", "-o", "a.o", "$SOURCEFILE"]
      },
      "run": {
        "cmd": "$OUTFILE"
      },
      "out": "a.o",
      "test": "sample.c"
    },
    "c++17": {
      "compile": {
        "cmd": "gcc",
        "opts": ["-std=c++17", "-Wall", "-Wextra", "-O3", "-o", "a.o", "$SOURCEFILE", "-lstdc++"]
      },
      "run": {
        "cmd": "$OUTFILE"
      },
      "out": "a.o"
    },
    "java": {
      "compile": {
        "cmd": "javac",
        "opts": ["$SOURCEFILE"]
      },
      "run": {
        "cmd": "java",
        "opts": ["-classpath", "$ROOTPATH", "$NAME"]
      },
      "out": "$NAME.class",
      "test": "Sample.java"
    },
    "python3": {
      "run": {
        "cmd": "python3",
        "opts": ["-S", "$OUTFILE"]
      },
      "test": "sample3.py"
    },
    "python2": {
      "run": {
        "cmd": "python2",
        "opts": ["-S", "$OUTFILE"]
      },
      "test": "sample2.py"
    }
  },
  "inExt": ".in",
  "outExt": ".out",
  "testsDir": "uploads/tests",
  "graderDir": "graders",
  "graders": ["default", "strict"],
  "defaultTestsuite": "global",
  "isolate": {
    "compileDefaults": {
      "time": 10,
      "wallTime": 10,
      "mem": 100000
    },
    "executeDefaults": {
      "time": 4,
      "wallTime": 40,
      "mem": 100000
    }
  }

}
