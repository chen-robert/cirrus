{
  "langs": {
    "c": {
      "compile": {
        "cmd": "gcc",
        "opts": ["-std=c11", "-Wall", "-Wextra", "-O3", "-lm", "-o", "a.o", "$SOURCEFILE"]
      },
      "run": {
        "cmd": "$OUTFILE"
      },
      "out": "a.o",
      "test": "sample.c"
    },
    "c++17": {
      "compile": {
        "cmd": "gcc",
        "opts": ["-std=c++17", "-Wall", "-Wextra", "-O3", "-o", "a.o", "$SOURCEFILE"]
      },
      "run": {
        "cmd": "$OUTFILE"
      },
      "out": "a.o"
    },
    "java": {
      "compile": {
        "cmd": "javac",
        "opts": ["$SOURCEFILE"]
      },
      "run": {
        "cmd": "java",
        "opts": ["-classpath", "$ROOTPATH", "$NAME"]
      },
      "out": "$NAME.class",
      "test": "Sample.java"
    },
    "python3": {
      "run": {
        "cmd": "python3",
        "opts": ["$OUTFILE"]
      },
      "test": "sample3.py"
    }
  },
  "inExt": ".in",
  "outExt": ".out",
  "testsDir": "uploads/tests",
  "graderDir": "graders",
  "graders": ["default", "strict"],
  "runTestsAsync": false,
  "defaultTestsuite": "global"
}